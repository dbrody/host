
<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.1/jquery.min.js"></script>
<h1>Maestro</h1>

<div id="real-time-data">
</div>

<script>
  function refreshStuff()
  {
    $.ajax({
      url: '/refresh',
      success: function(e) {
        $("#real-time-data").html(e);
        setTimeout("refreshStuff()", 1000);
      }
    });
  }

  $(function() {
    refreshStuff();
  });
</script>

